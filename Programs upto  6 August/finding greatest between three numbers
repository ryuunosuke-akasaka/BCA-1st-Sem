/*finding greatest between three numbers*/


#include<stdio.h>

int main()
{
int a,b,c;
printf("Enter three numbers ");
scanf("%d%d%d",&a,&b,&c);
if(a>b&&a>c)
  {
  printf("\n\n the greatest number is %d",a);
  }
else if(b>a&&b>c)
  {
   printf("\n\n the greatest number is %d",b);
  }
else {
      printf("\n\n the greatest number is %d",c);
     }
return 0;
  
}
